      assign imgg1 = 'https://cdn.shopify.com/s/files/1/0553/5802/9888/files/Snake_44c5f9c1-68a7-480d-bb0b-e60dd706560a_900x.jpg?v=1682568806'
      assign material_title1 = 'Our snake printed leather'
   assign material_link ='https://www.anoncph.com/collections/new-arrivals?sort_by=manual&filter.p.m.custom.material=Snake+calf&filter.p.m.custom.material=Snake+metallic+calf&filter.v.availability=1'
      assign material_content1 = '<p>Our snake printed leather is a textured and patterned leather that mimics the look and feel of real snake skin. This type of leather is perfect for adding a unique and exotic touch to any outfit. Our snake printed leather is sourced from high-quality tanneries that adhere to the standards set by the Leather Working Group (LWG) and REACH regulations, ensuring that the leather is produced using environmentally responsible and resource-efficient practices. Our snake printed leather is also durable and long-lasting, making it an excellent choice for shoes, bags, and other accessories.</p>'
      assign image_url_pattern1 = imgg1 | replace: '_720x.', '_{width}x.' 
  endif

  
  assign metallic_trim_section = 'no'
