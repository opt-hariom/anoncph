@font-face {
    font-family: 'Ogg';
    src:
        url('{{ "Ogg.eot" | asset_url }}') format('eot'),
        url('{{ "Ogg.woff" | asset_url }}') format('woff"),
        url('{{ "Ogg.woff2" | asset_url }}') format('woff2');
}

@font-face {   
    font-family: 'Ogg-Bold';
    src:
        url('https://cdn.shopify.com/s/files/1/0553/5802/9888/files/Ogg-Bold.eot') format('eot'),
        url('https://cdn.shopify.com/s/files/1/0553/5802/9888/files/Ogg-Bold.otf') format('otf'),
        url('https://cdn.shopify.com/s/files/1/0553/5802/9888/files/Ogg-Bold.ttf') format('ttf'),
        url('https://cdn.shopify.com/s/files/1/0553/5802/9888/files/Ogg-Bold.woff') format('woff'),
        url('https://cdn.shopify.com/s/files/1/0553/5802/9888/files/Ogg-Bold.woff2') format('woff2');
}

