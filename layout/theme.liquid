<script>
document.addEventListener('DOMContentLoaded', function() {
  // Initialize Flickity
  var flkty = new Flickity('.product-single__media-slider', {
    // Flickity options
    wrapAround: true
  });

  // Get the element for the current thumbnail number
  var currentThumb = document.getElementById('c_thumb');
  // Get the element for the last thumbnail number
  var lastThumb = document.getElementById('l_thumb');

  // Update the total number of slides
  lastThumb.textContent = flkty.slides.length;

  // Function to update the current thumbnail number
  function updateCurrentThumb() {
    currentThumb.textContent = flkty.selectedIndex + 1;
  }

  // Update the current thumbnail number on initial load
  updateCurrentThumb();

  // Update the current thumbnail number whenever the slide changes
  flkty.on('change', function(index) {
    updateCurrentThumb();
  });
});

</script>

    
  
