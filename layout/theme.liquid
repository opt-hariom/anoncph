<script type="lazyload2">
function aTooltip() {
  let abovePos = $(".above").offset();
  $(".dropAbove").css({"left": (abovePos.left - 123)+"px", "top": (abovePos.top + 32)+"px"});
  let windowWidth = $(window).width();
  if (windowWidth > 1199){
    $(".above").hover(function(){
      $(".dropAbove").show();
    })
    $(".dropAbove").hover(function(){
      $(this).show();
    })
    $("#shopify-section-header, #PageContainer").hover(function(){
      $(".dropAbove").hide();
    })
  } else if ( windowWidth <= 1199 ) {
    $(".above").click(function(){
      $(".dropAbove").toggle();
    })
    $("#shopify-section-header, #PageContainer").click(function(){
      $(".dropAbove").hide();
    })
  }
}

function drowerHeight(){ 
  let headerHeight = $("#shopify-section-header").height()+$("#Announcement--announcement-bar").height() - 1 + "px";
  let anHeight = $("#shopify-section-header").height() - 1 + "px";
  let anbarHeight = $("#Announcement--announcement-bar").height()   + "px";

  var scrollPos = $(window).scrollTop();
  if (scrollPos > 5) {
    $("#nav-drawer").css({"height": "calc(100vh - "+ anHeight + ")", "top": anHeight});
     if ($(window).width() > 768) {
        $("#cart-drawer").css({"height": "calc(100vh - "+ anHeight + ")", "top": anHeight});
       // $(".drawer__overlay").css({"top": anHeight});
     }
  } else {
    $("#nav-drawer").css({"height": "calc(100vh - "+ headerHeight + ")", "top": headerHeight});
    if ($(window).width() > 768) {
      $("#cart-drawer").css({"height": "calc(100vh - "+ headerHeight + ")", "top": headerHeight});
     // $(".drawer__overlay").css({"top": headerHeight});
    }
  }

  $(".nav-search").css({"top": anbarHeight});
}


setTimeout(function() {
  aTooltip();
  drowerHeight();
})

</script>
  
